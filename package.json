{
  "name": "GPT-LeetCode",
  "version": "1.2.0",
  "description": "GPT-LeetCode is an open-source project that leverages the power of GPT (Generative Pre-trained Transformer) to automatically solve algorithmic problems on LeetCode. With GPT-LeetCode, users can save time and effort by delegating the task of problem-solving to a powerful deep learning model. The project is designed to be easy to use and integrate into existing workflows, making it a valuable tool for developers, students, and anyone interested in improving their algorithmic problem-solving skills.",
  "scripts": {
    "build": "ts-node --project tsconfig.webpack.json -r tsconfig-paths/register ./node_modules/.bin/webpack --mode production",
    "dev": "ts-node --project tsconfig.webpack.json -r tsconfig-paths/register ./node_modules/.bin/webpack --mode development --watch"
  },
  "main": "index.tsx",
  "repository": "git@github.com:luzhenqian/gpt-leetcode.git",
  "author": "LuZhenqian <15753140326@163.com>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "highlight.js": "^11.8.0",
    "preact": "^10.13.2",
    "preact-router": "^4.1.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@types/chrome": "^0.0.235",
    "@types/glob": "^8.1.0",
    "@types/webpack": "^5.28.1",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "glob": "^10.2.2",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2"
  }
}
