{
  "plugins": [
    [
      "@babel/plugin-transform-typescript",
      {
        "isTSX": true
      }
    ],
    [
      "@babel/plugin-transform-react-jsx",
      {
        "runtime": "automatic",
        "importSource": "preact"
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 2 versions, > 1%"
      }
    ],
    "@babel/preset-react"
  ]
}